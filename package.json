{
  "name": "@reliverse/cleaner",
  "author": "reliverse",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "keywords": ["cli"],
  "description": "This CLI tool helps you easily clean up trash from your arrays.",
  "scripts": {
    "dev": "bun src/main.ts --file bang.ts",
    "check": "bun knip && bun typecheck && bun lint && bun format",
    "pub": "bun build.publish.ts",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --cache --fix .",
    "lint:i": "eslint --inspect-config",
    "format": "biome check --write .",
    "attw": "bunx @arethetypeswrong/cli",
    "unpub": "npm unpublish",
    "knip": "knip",
    "latest": "bun latest.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@reliverse/relico": "^1.0.1",
    "confbox": "^0.1.8",
    "destr": "^2.0.3",
    "execa": "^9.5.2",
    "fs-extra": "^11.3.0",
    "jiti": "^2.4.2",
    "jsdom": "^26.0.0",
    "magic-string": "^0.30.17",
    "pathe": "^2.0.3",
    "pkg-types": "^1.3.1",
    "semver": "^7.7.1",
    "tinyglobby": "^0.2.10"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@eslint/js": "^9.20.0",
    "@eslint/json": "^0.10.0",
    "@eslint/markdown": "^6.2.2",
    "@reliverse/config": "^1.5.6",
    "@stylistic/eslint-plugin": "^3.1.0",
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/bun": "^1.2.2",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.13.4",
    "@types/semver": "^7.5.8",
    "eslint": "^9.20.1",
    "eslint-plugin-no-relative-import-paths": "^1.6.1",
    "eslint-plugin-perfectionist": "^4.9.0",
    "knip": "^5.44.1",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.0",
    "unbuild": "^3.3.1",
    "vitest": "^3.0.7"
  }
}
